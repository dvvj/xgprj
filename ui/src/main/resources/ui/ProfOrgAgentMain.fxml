<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import com.jfoenix.controls.JFXTabPane?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml">
    <center>
        <JFXTabPane minWidth="1000.0">
            <Tab text="%orgagent.tab.profsTable.heading">
                <HBox>
                    <StackPane fx:id="profsTab">
                    </StackPane>
                    <VBox>
                        <VBox fx:id="vboxCostChartCurrProf" />
                        <VBox fx:id="vboxCostChartAll" />
                    </VBox>
                    <VBox>
                        <VBox fx:id="vboxRewardChartCurrProf" />
                        <VBox fx:id="vboxRewardChartAll" />
                    </VBox>
                </HBox>
            </Tab>
            <Tab text="%orgagent.tab.orderStatsTable.heading">
                <StackPane fx:id="orderStatsTab">
                </StackPane>
            </Tab>
            <Tab text="%orgagent.tab.rewardPlans.heading">
                <VBox fx:id="rewardPlanTab" />
            </Tab>
            <Tab text="%orgagent.tab.addNewProfs.heading">
                <VBox fx:id="addNewProfsTab" spacing="20">
                </VBox>
            </Tab>
            <Tab text="%orgagent.tab.updatePassword.heading">
                <VBox fx:id="updatePasswordTab" spacing="20">
                </VBox>
            </Tab>
        </JFXTabPane>
    </center>

</BorderPane>
